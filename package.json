{
  "name": "rn-google-cast",
  "title": "React Native Google Cast",
  "version": "1.0.1",
  "description": "React Native library to use Google Cast",
  "main": "lib/index",
  "types": "./lib/index",
  "scripts": {
    "prepublish": "tsc",
    "postpublish": "rimraf lib/**/*.js lib/**/*.d.ts",
    "clean:modules": "cd example && rimraf yarn.lock node_modules && yarn",
    "pod": "cd example/ios && pod install",
    "run:example": "npm run clean:modules && cd example && yarn android:dev",
    "lint:fix": "eslint lib/** --ext .js,.jsx,.ts,.tsx --fix"
  },
  "keywords": [
    "react-native",
    "android",
    "ios",
    "google-cast",
    "google-cast-sdk",
    "chromecast"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b3coded/rn-google-cast.git",
    "baseUrl": "https://github.com/b3coded/rn-google-cast"
  },
  "author": {
    "name": "JoÃ£o Alves",
    "email": "joaoalvs.tec@gmail.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": "^16.8.1",
    "react-native": ">=0.60.0 <1.0.x"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.8.3",
    "react-native": "^0.60.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
